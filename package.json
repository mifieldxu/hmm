{
    "name": "mm-hmm",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build-server": "tsc --project ./src/",
        "build-client": "tsc --project ./src/public/",
        "build-tests": "tsc --project ./src/tests/",
        "build": "npm run build-server && npm run build-client",
        "build-dev": "npm run build && npm run build-tests",
        "test": "npm run build-dev && mocha",
        "coverage": "nyc npm run test",
        "test:ci": "npm run build-dev && npx mocha",
        "coverage:ci": "npx nyc --reporter lcov npm run test:ci",
        "start": "npm run build && node ./dist/app"
    },
    "description": "mm-hmm",
    "author": "Mifield Xu",
    "main": "app.js",
    "dependencies": {
        "debug": "^2.2.0",
        "express": "^4.14.0",
        "http-status-codes": "^2.1.4",
        "pug": "^2.0.0-rc.3"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@tsconfig/node14": "^1.0.0",
        "@types/debug": "0.0.30",
        "@types/express": "^4.0.37",
        "@types/express-serve-static-core": "^4.0.50",
        "@types/mime": "^1.3.1",
        "@types/mocha": "^8.2.0",
        "@types/node": "^8.0.14",
        "@types/serve-static": "^1.7.32",
        "coveralls": "^3.1.0",
        "esm": "^3.2.25",
        "mocha": "^8.2.1",
        "nps": "^5.10.0",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "typescript": "^3.2.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mifieldxu/mm-hmm.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mifieldxu/mm-hmm/issues"
    },
    "homepage": "https://github.com/mifieldxu/mm-hmm#readme",
    "wallaby": {
        "autoDetect": true
    }
}
