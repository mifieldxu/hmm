{
    "name": "mm-hmm",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc --project ./src/tsconfig.json && tsc --project ./src/public/tsconfig.json",
        "clean": "tsc --project ./src/tsconfig.json --clean && tsc --project ./src/public/tsconfig.json --clean",
        "test": "npm run build && mocha ./dist/tests/run.js",
        "coverage": "nyc npm run test",
        "test:ci": "npm run build && ./node_modules/.bin/mocha ./dist/tests/run.js",
        "coverage:ci": "./node_modules/.bin/nyc npm run test:ci",
        "start": "node ./dist/app"
    },
    "description": "mm-hmm",
    "author": "Mifield Xu",
    "main": "app.js",
    "dependencies": {
        "debug": "^2.2.0",
        "express": "^4.14.0",
        "http-status-codes": "^2.1.4",
        "pug": "^2.0.0-rc.3"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@tsconfig/node14": "^1.0.0",
        "@types/debug": "0.0.30",
        "@types/express": "^4.0.37",
        "@types/express-serve-static-core": "^4.0.50",
        "@types/mime": "^1.3.1",
        "@types/node": "^8.0.14",
        "@types/serve-static": "^1.7.32",
        "coveralls": "^3.1.0",
        "esm": "^3.2.25",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "typescript": "^3.2.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mifieldxu/mm-hmm.git"
    },
