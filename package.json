{
  "name": "mm-hmm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestage":        "ts-node --skip-project ./src/.dev/.prestage.ts --all",
    "prestage.ci": "npx ts-node --skip-project ./src/.dev/.prestage.ts --all",
    "staging":    "npm run prestage    &&     tsc --project ./tsconfig.compile.package-scripts.json",
    "staging.ci": "npm run prestage.ci && npx tsc --project ./tsconfig.compile.package-scripts.json",
    "start":      "npm run staging    && nps",
    "start.ci":   "npm run staging.ci && nps",
    "test":       "npm start test",
    "test.ci":    "npm start test.ci",
    "precommit": "npm run prestage"
  },
  "description": "mm-hmm",
  "author": "Mifield Xu",
  "main": "./dist/app.js",
  "dependencies": {
    "chokidar": "^3.4.3",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "http-status-codes": "^2.1.4",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.16",
    "@types/fs-extra": "^9.0.5",
    "@types/js-yaml": "^3.12.5",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "@types/serve-static": "^1.13.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "esm": "^3.2.25",
    "js-yaml": "^3.14.1",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "nyc": "^15.1.0",
    "object-scan": "^13.6.9",
    "playwright": "^1.6.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "yaml-convert": "^1.0.0"
  },
  "repository": {
    "type": "git",
